ne":52,"column":40}},{"name":"e","generated":{"line":109,"column":38},"source":"Components/ImageUploader.js","original":{"line":52,"column":59}},{"generated":{"line":109,"column":39},"source":"Components/ImageUploader.js","original":{"line":52,"column":40}},{"generated":{"line":109,"column":40},"source":"Components/ImageUploader.js","original":{"line":52,"column":33}},{"generated":{"line":110,"column":0},"source":"Components/ImageUploader.js","original":{"line":52,"column":33}},{"generated":{"line":111,"column":0},"source":"Components/ImageUploader.js","original":{"line":50,"column":16}},{"generated":{"line":111,"column":24},"source":"Components/ImageUploader.js","original":{"line":40,"column":12}},{"generated":{"line":111,"column":25},"source":"Components/ImageUploader.js","original":{"line":39,"column":8}},{"generated":{"line":112,"column":0},"source":"Components/ImageUploader.js","original":{"line":56,"column":5}},{"name":"Component","generated":{"line":116,"column":2},"source":"Components/ImageUploader.js","original":{"line":3,"column":28}},{"name":"ImageUploader","generated":{"line":118,"column":15},"source":"Components/ImageUploader.js","original":{"line":60,"column":15}}],"sources":{"Components/ImageUploader.js":"import React, {Component} from \"react\";\r\n\r\nclass ImageUploader extends Component {\r\n\r\n    constructor(props) {\r\n        super(props);\r\n        this.state = {file: '', imagePreviewUrl: ''};\r\n    }\r\n\r\n    _handleSubmit(e) {\r\n        e.preventDefault();\r\n        console.log('handle uploading-', this.state.file);\r\n    }\r\n\r\n    _handleImageChange(e) {\r\n        e.preventDefault();\r\n\r\n        let reader = new FileReader();\r\n        let file = e.target.files[0];\r\n        reader.onloadend = () => {\r\n            this.setState({\r\n                file: file,\r\n                imagePreviewUrl: reader.result\r\n            });\r\n        }\r\n\r\n        reader.readAsDataURL(file)\r\n    }\r\n\r\n    render() {\r\n        let {imagePreviewUrl} = this.state;\r\n        let $imagePreview = null;\r\n        if (imagePreviewUrl) {\r\n            $imagePreview = (<img alt=\"imagePreview\" src={imagePreviewUrl}/>);\r\n        } else {\r\n            $imagePreview = (<div className=\"previewText\">Please select an Image for Preview</div>);\r\n        }\r\n\r\n        return (\r\n            <div className=\"previewComponent\">\r\n                <form onSubmit={(e) => this._handleSubmit(e)}>\r\n                    <input className=\"fileInput\"\r\n                           type=\"file\"\r\n                           onChange={(e) => this._handleImageChange(e)}/>\r\n                </form>\r\n                <div className=\"imgPreview\">\r\n                    {$imagePreview}\r\n                </div>\r\n\r\n                <button className=\"submitButton\"\r\n                        type=\"submit\"\r\n                        onClick={(e) => this._handleSubmit(e)}>Upload Image\r\n                </button>\r\n            </div>\r\n        )\r\n    }\r\n\r\n}\r\n\r\nexport default ImageUploader;"},"lineCount":null}},"error":null,"hash":"6cbcbfbe70ece7e2e261dc2524649460","cacheData":{"env":{}}}